<h1>Places with <%= params[:keyword]%> tag</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Zip</th>
      <th>Country</th>
    </tr>
  </thead>

  <tbody>
    <% @places.each do |place| %>
      <tr>
        <td><%= place.name %></td>
        <td><%= place.address %></td>
        <td><%= place.zip %></td>
        <td><%= place.country %></td>
      </tr>
    <% end %>
  </tbody>
</table>